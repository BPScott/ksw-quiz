{
  "name": "ksw-quiz",
  "version": "0.1.0",
  "homepage": "https://ksw-quiz.reload.me.uk/",
  "private": true,
  "dependencies": {
    "preact": "^10.1.0",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "5.0.1",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "clean": "rm -rf build",
    "type-check": "tsc",
    "predeploy": "npm run clean && npm run build",
    "deploy": "gh-pages -d build",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^11.10.4",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "gh-pages": "^2.1.1",
    "prettier": "^1.19.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "settings": {
      "react": {
        "version": "16.8"
      }
    }
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
