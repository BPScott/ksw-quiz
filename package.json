{
  "name": "ksw-quiz",
  "version": "0.1.0",
  "homepage": "https://ksw-quiz.reload.me.uk/",
  "private": true,
  "dependencies": {
    "preact": "^8.4.2",
    "preact-compat": "^3.18.4",
    "react-app-rewired": "^2.1.0",
    "react-scripts": "3.0.0",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "clean": "rm -rf build",
    "type-check": "tsc",
    "predeploy": "npm run clean && npm run build",
    "deploy": "gh-pages -d build",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css}'"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "gh-pages": "^1.2.0",
    "prettier": "^1.14.2"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
